{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/server/main.go",
      "cwd": "${workspaceFolder}",
      "env": {
        "CONFIG_PATH": "${workspaceFolder}/config/config.yaml",
        "EXCHANGE_RATE_FETCHER_URL": "EXCHANGE_RATE_FETCHER_URL",
        "EXCHANGE_RATE_FETCHER_KEY": "EXCHANGE_RATE_FETCHER_KEY"
      },
      "args": ["--host=localhost"],
      "showLog": true
    }
  ]
}
