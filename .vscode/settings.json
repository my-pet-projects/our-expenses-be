{
  "[yaml]": {
    "editor.autoIndent": "advanced"
  },
  "go.lintTool": "golangci-lint",
  "go.lintFlags": ["--fast"],
  "go.testFlags": ["-v"],
  "go.testEnvVars": {
    "CONFIG_PATH": "${workspaceFolder}/config/config.yaml"
  },
  "go.coverOnSave": true,
  "go.coverageDecorator": {
    "type": "gutter",
    "coveredHighlightColor": "rgba(64,128,128,0.5)",
    "uncoveredHighlightColor": "rgba(128,64,64,0.25)",
    "coveredGutterStyle": "blockgreen",
    "uncoveredGutterStyle": "blockred"
  },
  "go.coverOnSingleTest": true,
  "go.buildOnSave": "workspace",
  "cSpell.words": [
    "bcrypt",
    "bson",
    "checkstyle",
    "codegen",
    "donec",
    "dupl",
    "funlen",
    "gcloud",
    "gocognit",
    "goconst",
    "godotenv",
    "golangci",
    "golint",
    "govet",
    "gzipped",
    "healthz",
    "Infof",
    "joho",
    "mattn",
    "mongodb",
    "msgf",
    "nakedret",
    "nestif",
    "nlreturn",
    "oapi",
    "openapi",
    "otel",
    "otelecho",
    "otelmongo",
    "signup",
    "someword",
    "stretchr",
    "struct",
    "structs",
    "stylecheck",
    "testpackage",
    "unmarshalled",
    "Upserted",
    "Warnf",
    "YYOS"
  ]
}
